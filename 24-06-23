
class Solution{   
public:
    int klengthpref(string arr[], int n, int k, string str){    
        int count1 = 0;
        for(int i =0; i<n; i++){
          int count =0;
            for(int j=0; j<k; j++){
            if(str[j] == (arr[i])[j]){
                count ++;
            }
            }if(count >= k){
            count1++;
            }
        }
        return count1;
    }
};
